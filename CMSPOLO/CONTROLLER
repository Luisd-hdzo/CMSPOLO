from CMSPOLO import model

class AppController:
    def __init__(self):
        self.model = model

    def agregar_persona(self, nombre):
        self.model.agregar_persona(nombre)

    def agregar_evento(self, nombre_evento, nombre_organizador):
        self.model.agregar_evento(nombre_evento, nombre_organizador)

    def agregar_relacion(self, nombre1, nombre2):
        self.model.agregar_relacion(nombre1, nombre2)

    def buscar_relaciones(self, nombre):
        return self.model.buscar_relaciones(nombre)
